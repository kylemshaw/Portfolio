<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbProducts" style="-fx-background-color: pink;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1">
   <children>
       <TableView fx:id="tvTableName" layoutX="20.0" layoutY="28.0" maxWidth="-Infinity" prefHeight="400.0" prefWidth="572.0">
           <columns>
               <TableColumn fx:id="colItemId" prefWidth="102.0" styleClass="tableColumn" text="ID" />
               <TableColumn fx:id="colItemName" prefWidth="468.0" styleClass="tableColumn" text="Name" />
           </columns>
       </TableView>
      <HBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="55.0" prefWidth="572.0">
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
         <children>
            <Button fx:id="btnAdd" mnemonicParsing="false" text="Add">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="btnEdit" disable="true" mnemonicParsing="false" prefHeight="30.0" prefWidth="59.0" text="Edit">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnDelete" disable="true" mnemonicParsing="false" text="Delete">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="35.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
