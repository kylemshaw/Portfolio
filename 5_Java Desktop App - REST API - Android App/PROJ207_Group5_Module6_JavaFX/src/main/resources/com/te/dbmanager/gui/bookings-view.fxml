<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbProducts" alignment="CENTER" stylesheets="@Snow_style.css" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.te.dbmanager.gui.BookingController">
   <children>
      <TableView fx:id="tableBookings" layoutX="4.0" maxWidth="-Infinity" prefHeight="555.0" prefWidth="915.0">
         <columns>
            <TableColumn fx:id="colBookingId" prefWidth="93.0" text="C1" />
            <TableColumn fx:id="colBookingDate" prefWidth="116.0" styleClass="colCenter" text="C2" />
            <TableColumn fx:id="colBookingNo" prefWidth="102.0" text="Column X" />
            <TableColumn fx:id="colTravelerCount" prefWidth="111.0" styleClass="colCenter" text="Column X" />
            <TableColumn fx:id="colCustomerId" prefWidth="100.0" styleClass="colCenter" text="Column X" />
            <TableColumn fx:id="colTripTypeId" minWidth="9.0" prefWidth="96.0" styleClass="colCenter" text="Column X" />
            <TableColumn fx:id="colPackageId" prefWidth="101.0" styleClass="colCenter" text="Column X" />
            <TableColumn fx:id="colFeeId" prefWidth="75.0" styleClass="colCenter" text="Column X" />
            <TableColumn fx:id="colAgentId" prefWidth="90.0" styleClass="colCenter" text="Column X" />
         </columns>
      </TableView>
      <HBox alignment="CENTER_LEFT" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="90.0" prefWidth="915.0">
         <padding>
            <Insets bottom="15.0" left="15.0" right="15.0" top="15.0" />
         </padding>
         <children>
            <Button fx:id="btnAdd" minWidth="68.0" mnemonicParsing="false" text="Add">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Pane HBox.hgrow="ALWAYS" />
            <Button fx:id="btnEdit" disable="true" minWidth="68.0" mnemonicParsing="false" prefHeight="30.0" prefWidth="68.0" text="Edit">
               <font>
                  <Font size="14.0" />
               </font>
            </Button>
            <Button fx:id="btnDelete" disable="true" minWidth="68.0" mnemonicParsing="false" prefWidth="68.0" text="Delete">
               <font>
                  <Font size="14.0" />
               </font>
               <HBox.margin>
                  <Insets left="35.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
